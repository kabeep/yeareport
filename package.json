{
    "name": "annualize",
    "version": "0.4.4",
    "description": "Node.js CLI based on git commit for annual contribute index, generates Markdown",
    "keywords": [
        "annual contribution",
        "git commit",
        "git log",
        "cli",
        "nodejs"
    ],
    "author": "Jason Zhang (kabeep)",
    "homepage": "https://github.com/kabeep/annualize#readme",
    "repository": "git@github.com:kabeep/annualize.git",
    "license": "MIT",
    "main": "./dist/cli.js",
    "types": "./dist/cli.d.ts",
    "files": [
        "dist"
    ],
    "bin": {
        "annualize": "dist/cli.js"
    },
    "scripts": {
        "prepare": "husky install",
        "dev": "npm run build:dev && node bin/cli.ts",
        "build:dev": "tsup",
        "build": "tsup --dts",
        "test": "xo src/**/*.ts bin/**/*.js --fix"
    },
    "devDependencies": {
        "@swc/core": "^1.3.102",
        "@types/chalk": "^2.2.0",
        "@types/cli-spinner": "^0.2.3",
        "@types/inquirer": "^9.0.7",
        "@types/json2md": "^1.5.4",
        "@types/lodash.intersection": "^4.4.9",
        "@types/lodash.startcase": "^4.4.9",
        "@types/node": "^20.11.0",
        "@types/yargs": "^17.0.32",
        "husky": "^8.0.3",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3",
        "xo": "^0.56.0"
    },
    "dependencies": {
        "@kabeep/lunar-date-fns": "^1.1.0",
        "@node-rs/jieba": "^2.0.1",
        "chalk": "4.1.2",
        "cli-spinner": "^0.2.10",
        "json2md": "^2.0.3",
        "linebyline": "^1.3.0",
        "lodash.intersection": "^4.4.0",
        "lodash.startcase": "^4.4.0",
        "yargs": "^17.7.2"
    }
}
