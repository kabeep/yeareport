    "@types/lodash.startcase": "^4.4.9",
    "@types/node": "^20.11.0",
    "@types/yargs": "^17.0.32",
    "husky": "^8.0.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "linebyline": "^1.3.0",
    "lodash.intersection": "^4.4.0",
    "lodash.startcase": "^4.4.0",
